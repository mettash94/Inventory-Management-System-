<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AddPartFormController">
   <children>
      <Label layoutX="50.0" layoutY="36.0" text="Add Part">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <RadioButton fx:id="InHouseRadio" layoutX="267.0" layoutY="37.0" mnemonicParsing="false" onAction="#ActionInHousePart" selected="true" text="InHouse">
         <toggleGroup>
            <ToggleGroup fx:id="ToggleButton" />
         </toggleGroup></RadioButton>
      <RadioButton fx:id="OutSourcedRadio" layoutX="417.0" layoutY="37.0" mnemonicParsing="false" onAction="#ActionOutSourcedPArt" text="Out Sourced" toggleGroup="$ToggleButton" />

      <Text layoutX="79.0" layoutY="123.0" strokeType="OUTSIDE" strokeWidth="0.0" text="ID">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Text>
      <Text layoutX="79.0" layoutY="160.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Name">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Text>
      <Text layoutX="82.0" layoutY="195.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Inv" wrappingWidth="22.0">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Text>
      <Text layoutX="77.0" layoutY="229.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Price">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Text>
      <Text layoutX="79.0" layoutY="262.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Max">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Text>
      <TextField disable="true" layoutX="138.0" layoutY="105.0" text="AutoGenerated" />
      <TextField fx:id="inputPartName" layoutX="138.0" layoutY="141.0" />
      <TextField fx:id="inputPartInventory" layoutX="138.0" layoutY="176.0" />
      <TextField fx:id="inputPartPrice" layoutX="138.0" layoutY="210.0" />
      <TextField fx:id="inputPartMachineId" layoutX="166.0" layoutY="282.0" />
      <TextField fx:id="inputPartMin" layoutX="375.0" layoutY="244.0" />
      <Text layoutX="332.0" layoutY="261.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Min">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Text>
      <Text fx:id="MachineIdOrCompanyName" layoutX="56.0" layoutY="301.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Machine ID">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Text>
      <TextField fx:id="inputPartMax" layoutX="138.0" layoutY="244.0" />
      <Button fx:id="SavePartButton" layoutX="332.0" layoutY="331.0" mnemonicParsing="false" onAction="#ActionSavePartData" text="SAVE" />
      <Button fx:id="CancelButton" layoutX="395.0" layoutY="331.0" mnemonicParsing="false" onAction="#ActionCancelPart" text="CANCEL" />
   </children>
</AnchorPane>
